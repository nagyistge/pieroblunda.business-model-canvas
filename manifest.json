{
  "name": "Business Model Canvas",
  "short_name": "pbCanvas",
  "description": "html5 Bussines Model Canvas",
  "version": "1.1.1",
  "manifest_version": 2,
  "icons": {
     "16": "icons/icon16.png",
     "128": "icons/icon128.png",
     "256": "icons/icon256.png"
  },
  "app": {
    "launch": {
      "local_path": "layout.html"
    }
  },
  "permissions": [
    "app.window",
    "<all_urls>",
    "storage"
  ],
  "app": {
    "background": {
      "scripts": [
        "js/main.js"
      ]
    }
  }
}